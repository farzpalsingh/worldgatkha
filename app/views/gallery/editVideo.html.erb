<div class="container-fluid">
	<!-- Breadcrumbs-->
	<ol class="breadcrumb">
	    <li class="breadcrumb-item">
	    	<%= link_to "Dashboard", admin_index_path %>
	    </li>
	    <li class="breadcrumb-item">Gallery</li>
	    <li class="breadcrumb-item active">Edit Video</li>
	</ol>

	<!-- Page Content -->
	<h3>Edit Video</h3>
	<hr>
	<div class="addNewsForm">
		<%= form_with scope: :gallery_video, :url => admin_gallery_video_path, method: :PATCH, local: true do |f| %>
            <div class="form-group">
              	<div class="form-label-group-new">
	              	<%= f.label :title %>
	                <%= f.text_field :title, autofocus: true, autocomplete: "title", class: 'form-control', placeholder: "Enter Video Title" %>
              	</div>
            </div>
            
            <div class="form-group">
              	<div class="form-label-group-new">
	              	<%= f.label :video_url %>
	                <%= f.text_field :video_url, autofocus: true, autocomplete: "video_url", class: 'form-control', placeholder: "Enter Video Url" %>
	            </div>
            </div>

            <div class="actions">
              	<%= f.submit "Save Video", :class => 'btn btn-primary btn-block' %>
            </div>
        <% end %>
	</div>
</div>
<!-- /.container-fluid -->